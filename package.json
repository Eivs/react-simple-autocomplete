{
  "name": "react-simple-autocomplete",
  "version": "1.0.0",
  "description": "A simple, customizable wrapper to provide autocomplete functionality to inputs",
  "main": "build/simple-autocomplete.min.js",
  "scripts": {
    "clean": "rimraf lib build dist",
    "lint": "eslint src",
    "build": "cross-env NODE_ENV=production npm-run-all clean build:lib build:umd build:dist",
    "build:lib": "babel src -d lib",
    "build:umd": "webpack --progress --config=webpack.umd.js",
    "build:dist": "webpack --progress --config=webpack.dist.js",
    "test": "NODE_ENV=test mocha --opts mocha.opts src/**/*-tests.js",
    "start": "node server.dev.js"
  },
  "author": "Ben Yorke",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "http://github.com/bendyorke/react-simple-autocomplete.git"
  },
  "dependencies": {
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "cssnano": "^3.4.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.15.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "ignore-styles": "^1.2.0",
    "mocha": "^2.3.4",
    "node-jsdom": "^3.1.5",
    "npm-run-all": "^1.5.0",
    "postcss-color-function": "^2.0.0",
    "postcss-custom-properties": "^5.0.0",
    "postcss-loader": "^0.8.0",
    "postcss-nested": "^1.0.0",
    "react-addons-test-utils": "^0.14.6",
    "rimraf": "^2.5.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.6.0"
  }
}
